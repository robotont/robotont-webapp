<?xml version="1.0" ?>
<launch>
  <node name="rosbridge_websocket" pkg="rosbridge_server" type="rosbridge_websocket" args="port:=9090"/>
  <node name="web_video_server" pkg="web_video_server" type="web_video_server" args="_port:=4000"/>
  <node name="tf2_web_republisher" pkg="tf2_web_republisher" type="tf2_web_republisher"/>
  <node name="depthcloud_encoder_node" pkg="depthcloud_encoder" type="depthcloud_encoder_node" args="_depth:=/camera/depth/image_rect_raw _rgb:=/camera/color/image_raw"/>
  <node name="nodejs_app" pkg="robotont_webapp" type="npm_script.sh" output="screen"/>
  <node name="ttyd_terminal" pkg="robotont_webapp" type="ttyd_script.sh" output="screen"/>
</launch>
